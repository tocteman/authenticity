{
  "name": "gatsby-starter-tailwind",
  "description": "Gatsby starter styled with Tailwind",
  "version": "1.1.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "gatsby": "^1.9.261",
    "gatsby-image": "^1.0.51",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "gatsby-plugin-sharp": "^1.6.44",
    "gatsby-source-filesystem": "^1.5.36",
    "gatsby-transformer-sharp": "^1.6.24",
    "react-headroom": "^2.2.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "typeface-eb-garamond": "0.0.54",
    "typeface-ibm-plex-mono": "0.0.56",
    "typeface-old-standard-tt": "0.0.54",
    "typeface-source-serif-pro": "0.0.54"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tailwind build ./src/layouts/index.tailwind.css -c ./tailwind.js -o ./src/layouts/index.css && purgecss -c purgecss.config.js -o ./src/layouts && gatsby build",
    "develop": "tailwind build ./src/layouts/index.tailwind.css -c ./tailwind.js -o ./src/layouts/index.css && gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.11.1",
    "purgecss": "^0.21.0",
    "tailwindcss": "^0.5.1"
  }
}
